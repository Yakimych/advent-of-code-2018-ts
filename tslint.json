{
    "extends": ["tslint:recommended"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
      "interface-over-type-literal": false,
      "object-literal-sort-keys": false,
      "jsx-no-lambda": false
    }
}
