{
  "extends": ["tslint:recommended"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "arrow-parens": false,
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  }
}
